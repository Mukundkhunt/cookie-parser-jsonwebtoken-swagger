    "host": "petstore.swagger.io",
    "basPath": "v2",
    "tags": [{
            "name": "User",
            "description": "Everything about your Food"
        },
        {
            "name": "Project",
            "description": "What you want to eat"
        }
    ],
    "schemes": ["http"],
    "paths": {
        "/user": {
            "get": {
                "tags": ["User"],
                "summary": "Get all the user",
                "parameters": [{
                    "in": "body",
                    "name": "body",
                    "description": "Email is required",
                    "schema": {
                        "required": ["email"],
                        "properties": {
                            "email": {
                                "type": "string"
                            }
                        },
                        "example": {
                            "email": "khuntmukund123@gmail.com"
                        }
                    }
                }],
                "response": {}
            }
        },
        "/user/create": {
            "post": {
                "tags": ["User"],
                "summary": "Create User Account",
                "parameters": [{
                    "in": "body",
                    "name": "body",
                    "description": "Unique Email address and Password",
                    "schema": {
                        "required": ["email", "password"],
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "email": {
                                "type": "string"
                            },
                            "password": {
                                "type": "string"
                            }
                        },
                        "example": {
                            "name": "Mukund Khunt",
                            "email": "Mukund.semicolon@gmail.com",
                            "password": "my-secret-password"

                        }
                    }
                }],
                "responses": {}
            }
        }
    }

